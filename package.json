{
  "name": "link-autotitle",
  "version": "1.0.0",
  "description": "Automatically converts URLs to markdown links with page titles",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "obsidian.md",
    "obsidian",
    "link-autotitle"
  ],
  "author": "widnyana <wid@widnyana.web.id>",
  "license": "WTFPL",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.8.1",
    "typescript": "5.7.2"
  }
}
